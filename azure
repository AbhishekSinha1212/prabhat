
CREATE TABLE E_1
(
	a1 char(18) NOT NULL 
);

CREATE TABLE E_2
(
	b1 char(18) NOT NULL ,
	a1 char(18) NOT NULL 
);

CREATE TABLE E_3
(
	c1 char(18) NOT NULL ,
	b1 char(18) NULL ,
	a1 char(18) NULL 
);

CREATE TABLE E_4
(
	d1 char(18) NOT NULL 
);

ALTER TABLE E_1
	ADD CONSTRAINT XPKE_1 PRIMARY KEY NONCLUSTERED (a1 ASC) NOT ENFORCED ;

ALTER TABLE E_2
	ADD CONSTRAINT XPKE_2 PRIMARY KEY NONCLUSTERED (b1 ASC,a1 ASC) NOT ENFORCED ;

ALTER TABLE E_3
	ADD CONSTRAINT XPKE_3 PRIMARY KEY NONCLUSTERED (c1 ASC) NOT ENFORCED ;

ALTER TABLE E_4
	ADD CONSTRAINT XPKE_4 PRIMARY KEY NONCLUSTERED (d1 ASC) NOT ENFORCED ;

CREATE VIEW V_1
(
	col1,
	col2
)
AS 
