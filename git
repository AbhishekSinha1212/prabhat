
CREATE TABLE ModelType
(
	Name                 CHAR(18) NOT NULL,
	Version              CHAR(18) NULL
);

ALTER TABLE ModelType
ADD PRIMARY KEY (Name);

CREATE TABLE PhysicalOnly
(
	TestA                CHAR(18) NOT NULL,
	TestB                CHAR(18) NULL,
	Name                 CHAR(18) NOT NULL
);

ALTER TABLE PhysicalOnly
ADD PRIMARY KEY (TestA,Name);

ALTER TABLE PhysicalOnly
ADD CONSTRAINT R_1 FOREIGN KEY (Name) REFERENCES ModelType (Name);
