
CREATE TABLE E_1
(
	a1 CHAR(18) NOT NULL 
);

ALTER TABLE E_1
	ADD CONSTRAINT PRIMARY KEY  ( a1 ASC );

CREATE TABLE E_2
(
	b1 CHAR(18) NOT NULL ,
	a1 CHAR(18) NOT NULL 
);

ALTER TABLE E_2
	ADD CONSTRAINT PRIMARY KEY  ( b1 ASC, a1 ASC );

CREATE TABLE E_3
(
	c1 CHAR(18) NOT NULL ,
	b1 CHAR(18) NULL ,
	a1 CHAR(18) NULL 
);

ALTER TABLE E_3
	ADD CONSTRAINT PRIMARY KEY  ( c1 ASC );

CREATE TABLE E_4
(
	d1 CHAR(18) NOT NULL 
);

ALTER TABLE E_4
	ADD CONSTRAINT PRIMARY KEY  ( d1 ASC );

ALTER TABLE E_2
	ADD  CONSTRAINT R_1 FOREIGN KEY (a1) REFERENCES E_1 (a1);

ALTER TABLE E_3
	ADD  CONSTRAINT R_2 FOREIGN KEY (b1, a1) REFERENCES E_2 (b1, a1);

CREATE VIEW V_1 (col1,
col2)
AS 
SELECT Expr_154,Expr_155
FROM V_1 ;
