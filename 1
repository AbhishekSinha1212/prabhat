
CREATE TABLE Concurrent_Curricula
(
	personid             NUMERIC(8)
		LABEL = 'Person ID',
	LMOD                 CHARACTER(20)
		LABEL = 'LMOD',
	StuProgram1_inTerm   CHARACTER(48)
		LABEL = 'Stu Program 1 (in Term)',
	StuProgram2_InTerm   CHARACTER(48)
		LABEL = 'Stu Program 2 (In Term)',
	SID_ConcurrentCurricula NUMERIC(8)
		LABEL = 'SID-Concurrent Curricula' NOT NULL ,
	StuMajorCd11_inTerm  CHARACTER(16)
		LABEL = 'Stu Major Cd 1-1 (in Term)',
	StuMajorCd12_inTerm  CHARACTER(16)
		LABEL = 'Stu Major Cd 1-2 (in Term)',
	StuMajorCd21_inTerm  CHARACTER(16)
		LABEL = 'Stu Major Cd 2-1 (in Term)',
	StuMajorCd22_inTerm  CHARACTER(16)
		LABEL = 'Stu Major Cd 2-2 (in Term)',
	CurrTermEffect       CHARACTER(24)
		LABEL = 'Curr Term Effect',
	CurrNextTermEffect   CHARACTER(24)
		LABEL = 'Curr Next Term Effect'
);

CREATE UNIQUE INDEX XAK1Concurrent_Curricula ON Concurrent_Curricula
(
	LMOD ASC,
	personid ASC,
	CurrTermEffect ASC
);

CREATE TABLE E_161
(
	_default_            CHARACTER(18)
		LABEL = 'test' NOT NULL ,
	demo                 CHARACTER(18)
		LABEL = '<default>'
);

CREATE TABLE Student_Dim
(
	personid             NUMERIC(8)
		LABEL = 'Person ID' NOT NULL ,
	V_ID                 CHARACTER(36)
		LABEL = 'Stu VID',
	SGBSTDN_ADMT_CODE    CHARACTER(8)
		LABEL = 'Stu Admin Type Cd',
	STGBSTDN_DEGC_CODE_1 CHARACTER(24)
		LABEL = 'Stu Degree Cd1',
	SGBSTDN_DEGC_CODE_2  CHARACTER(24)
		LABEL = 'Stu Degree Cd2',
	SGBSTDN_EXP_GRAD_DATE DATE
		LABEL = 'Stu Exp Grad Dt',
	SDBSTDN_MAJR_CODE_1  CHARACTER(16)
		LABEL = 'Stu Major Cd1 (Obsolete)',
	SGBSTDN_MAJR_CODE_1_2 CHARACTER(16)
		LABEL = 'Stu Major Cd1-2 (Obsolete)',
	SGBSTDN_MAJR_CODE_2  CHARACTER(16)
		LABEL = 'Stu Major Cd2 (Obsolete)',
	SGBSTDN_MAJR_CODE_2_2 CHARACTER(16)
		LABEL = 'Stu Major Cd2-2 (Obsolete)',
	SGBSTDN_PROGRAM_1    CHARACTER(48)
		LABEL = 'Stu Program1',
	SGBSTDN_PROGRAM_2    CHARACTER(48)
		LABEL = 'Stu Program2',
	SGBSTDN_SECD_ROLL_IND CHARACTER(4)
		LABEL = 'Stu Secd Roll Ind',
	SGBSTDN_SITE_CODE    CHARACTER(12)
		LABEL = 'Stu Site Cd',
	SGBSTDN_STST_CODE    CHARACTER(8)
		LABEL = 'Stu Status Cd',
	SHRTCKG_TERM_CODE    CHARACTER(24)
		LABEL = 'Stu First Grade Term Cd',
	STREET1              CHARACTER(300)
		LABEL = 'Stu Addr 1',
	STREET2              CHARACTER(300)
		LABEL = 'Stu Addr 2',
	STREET3              CHARACTER(300)
		LABEL = 'Stu Addr 3',
	CITY                 CHARACTER(200)
		LABEL = 'Stu City',
	STATE                CHARACTER(12)
		LABEL = 'Stu State Cd',
	ZIP                  CHARACTER(120)
		LABEL = 'Stu ZIP',
	COUNTY               CHARACTER(5)
		LABEL = 'Stu County Cd',
	Email                CHARACTER(20)
		LABEL = 'Stu email',
	Atlas                CHARACTER(120)
		LABEL = 'Stu Atlas email',
	St_FTIC_Trm          CHARACTER(24)
		LABEL = 'Stu FTIC Term Cd',
	LAST_NAME            CHARACTER(240)
		LABEL = 'Stu Name (L)',
	FIRST_NAME           CHARACTER(240)
		LABEL = 'Stu Name (F)',
	SPRIDEN_MI           CHARACTER(240)
		LABEL = 'Stu Name (MI)',
	studentlfm           CHARACTER(240)
		LABEL = 'Stu Name (L, F, MI)',
	Dead_Ind             CHARACTER(4)
		LABEL = 'Stu Deceased Ind',
	sdt_hs_cd            CHARACTER(24)
		LABEL = 'Stu Grad HS Cd',
	sdt_hs               CHARACTER(120)
		LABEL = 'Stu Grad HS',
	Area                 CHARACTER(24)
		LABEL = 'Stu Area Cd',
	Phone                CHARACTER(20)
		LABEL = 'Stu Phone Nbr',
	Ext                  CHARACTER(20)
		LABEL = 'Stu Phone Ext',
	StuCounty            CHARACTER(30)
		LABEL = 'Stu County',
	StuBirthdate         DATE
		LABEL = 'Stu BirthDate',
	StuAge               NUMERIC(8)
		LABEL = 'Stu Age'
);

