ALTER TABLE Concurrent_Curricula
ADD CONSTRAINT XPKConcurrent_Curricula PRIMARY KEY (SID_ConcurrentCurricula);

ALTER TABLE E_161
ADD CONSTRAINT XPKE_161 PRIMARY KEY (_default_);

ALTER TABLE Student_Dim
ADD CONSTRAINT XPKStudent PRIMARY KEY (personid);

ALTER TABLE Student_End_Term
ADD CONSTRAINT XPKStudent_End_Term PRIMARY KEY (PIDM_EndTermCd);

ALTER TABLE Student_Start_Term
ADD CONSTRAINT XPKStudent_Start_Term PRIMARY KEY (PIDM_StartTermCd);

ALTER TABLE Student_Term_Dim
ADD CONSTRAINT XPKStudent_Term PRIMARY KEY (personid_term_cd);
